{
    "name": "jandersongarcia/mobi-php",
    "description": "Desenvolvimento de aplicações em componentes",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Janerson Garcia",
            "email": "jganjos.info@gmail.com"
        }
    ],
    "homepage": "https://github.com/jandersongarcia/mobiphp",
    "autoload": {
        "classmap": [
            "core/class"
        ]
    },
    "version": "1.0.0",
    "require": {
        "php": ">=7.0",
        "matthiasmullie/minify": "^1.3",
        "twbs/bootstrap": "5.3.2",
        "twbs/bootstrap-icons": "^1.10"
    },
    "scripts": {
        "mobi-create-page": "php core/php/composer/create-page.php",
        "mobi-delete-page": "php core/php/composer/delete-page.php",
        "mobi-create-component": "php core/php/composer/create-component.php",
