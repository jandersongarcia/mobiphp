{
    "name": "jandersongarcia/mobi-php",
    "description": "Desenvolvimento de aplicações em componentes",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Janerson Garcia",
            "email": "jganjos.info@gmail.com"
        }
    ],
    "homepage": "https://github.com/jandersongarcia/mobiphp",
    "autoload": {
        "classmap": [
            "core/class"
        ]
    },
    "version": "1.0.1",
    "require": {
        "php": ">=7.0",
        "matthiasmullie/minify": "^1.3",
        "twbs/bootstrap": "5.3.2",
        "twbs/bootstrap-icons": "^1.10"
    },
    "config": {
        "vendor-dir": "minha-pasta"
    },
    "scripts": {
        "post-install-cmd": [
            "cp -r minha-pasta/core/class/* ./",
            "php minha-pasta/core/php/composer/create-page.php",
            "php minha-pasta/core/php/composer/delete-page.php",
            "php minha-pasta/core/php/composer/create-component.php",
            "php minha-pasta/core/php/composer/delete-component.php",
            "php minha-pasta/core/php/composer/create-module.php",
            "php minha-pasta/core/php/composer/delete-module.php",
            "php minha-pasta/core/php/composer/routes-list.php",
            "php minha-pasta/core/php/composer/routes-rename.php"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
